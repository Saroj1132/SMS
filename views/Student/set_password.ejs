                                                <div class="row mt-3"> 
                                                    <div class="col-md-12">
                                                        <fieldset class="form-group"> 
                                                            <label class="form-lbl">Passwrod<span style="color:red">*</span></label>
                                                            <input type="password" name="Password" class="form-control" id="Password">
                                                        </fieldset>
                                                        <% if(error.Password) { %>
                                                            <p style="color:red; font-weight: bolder;"><%= error.Password.msg %></p>
                                                    
                                                            <% }  %>
                                                            <p style="color: red; font-weight: bolder;"><%=Match%></p>
                                                    
                                                        
                                                    </div>  
                                                    <div class="col-md-12">
                                                        <fieldset class="form-group"> 
                                                            <label class="form-lbl">Confirm Password<span style="color:red">*</span></label>
                                                            <input type="password" name="CPassword" class="form-control" id="">
                                                        </fieldset>
                                                        <% if(error.CPassword) { %>
                                                            <p style="color:red; font-weight: bolder;"><%= error.CPassword.msg %></p>
                                                    
                                                            <% }  %>
                                                            <p style="color: red; font-weight: bolder;"><%=Match%></p>                                               
                                                        
                                                    </div>   
                                                </div> 


                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="mt-2" align="center"> 
                                                            <p style="color: red; font-weight: bolder;"><%=Invalid%></p>
                                                            <button class="round-btn d-block">Change Password</button>
                                                         </div>
                                                    </div>
                                                    <div class="col-md-12 mt-4">
